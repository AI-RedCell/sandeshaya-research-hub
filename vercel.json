{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "redirects": [
    {
      "source": "/survey-entry",
      "destination": "/survey-entry/login.html",
      "permanent": false
    },
    {
      "source": "/survey-entry/",
      "destination": "/survey-entry/login.html",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/survey-entry/(.*)",
      "destination": "/survey-entry/$1"
    },
    {
      "source": "/dashboard/(.*)",
      "destination": "/dashboard/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}